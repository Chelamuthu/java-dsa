import java.util.*;

public class sumOfDigit {
  
  static int sum(int a){
    if(a<10)
    return a;
    
    
    return (a%10)+sum(a/10) ;
    
  }
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int a=sc.nextInt();
      System.out.println(sum(a));
    }
}